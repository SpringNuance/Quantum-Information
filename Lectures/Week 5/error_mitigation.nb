   RowBox[{"\[Rho]_", ",", "\[Epsilon]_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    FractionBox["\[Epsilon]", "2"], 
    RowBox[{"IdentityMatrix", "[", "2", "]"}]}], "+", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"1", "-", "\[Epsilon]"}], ")"}], 
