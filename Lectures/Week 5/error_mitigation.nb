9d6bf6db683b"],

Cell[TextData[{
 "In general the mitigated estimate for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
    RowBox[{"(", "0", ")"}]}], TraditionalForm]],ExpressionUUID->
  "2f62f1f7-63d1-4165-be61-18ce2909de12"],
 " is\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
     RowBox[{"(", "0", ")"}]}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"k", "=", "0"}], "n"], 
     RowBox[{
      SubscriptBox["\[Beta]", "k"], 
      RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Alpha]", "k"], "\[Epsilon]"}], ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"b6d6c5e0-99da-44ab-9af5-41aaf5a731c6"],
 " ,\nwhere\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Beta]", "k"], "=", 
    RowBox[{
     SubscriptBox["\[Product]", 
      RowBox[{"i", "\[NotEqual]", "k"}]], 
     FractionBox[
      SubscriptBox["\[Alpha]", "i"], 
      RowBox[{
       SubscriptBox["\[Alpha]", "k"], "-", 
       SubscriptBox["\[Alpha]", "i"]}]]}]}], TraditionalForm]],ExpressionUUID->
  "0ea03b50-2113-4e2e-8278-b6ec4e7f92c8"],
 " ."
}], "Text",
 CellChangeTimes->{{3.8436561347441998`*^9, 
  3.8436562340611753`*^9}},ExpressionUUID->"f1752163-42cc-4681-8946-\
5af404f5363a"]
}, Open  ]],

