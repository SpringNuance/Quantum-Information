Cell[CellGroupData[{
Cell["Error mitigation via extrapolation", "Title",
 CellChangeTimes->{{3.843649407074306*^9, 
  3.843649424370298*^9}},ExpressionUUID->"0065fd0e-74e0-4743-8ddd-\
b1c630e11ec0"],

Cell["Reference: quant-ph/2011.01382", "Text",
 CellChangeTimes->{{3.8436562883674183`*^9, 
  3.843656293158411*^9}},ExpressionUUID->"cb7dd03d-a56a-4546-baca-\
9aed7863fd46"],

Cell[CellGroupData[{

Cell["Richardson extrapolation", "Chapter",
 CellChangeTimes->{{3.843649431810444*^9, 
  3.843649436335361*^9}},ExpressionUUID->"76f0ee80-5802-48bd-834b-\
f4aa66f5e03b"],

Cell[TextData[{
 "For simplicity, consider that each gate has the same error rate ",
 Cell[BoxData[
  FormBox["\[Epsilon]", TraditionalForm]],ExpressionUUID->
  "1cc428d1-1115-4041-8277-d18cea29f2ff"],
 ". Then the expectation value of some observable ",
 Cell[BoxData[
  FormBox["M", TraditionalForm]],ExpressionUUID->
  "866d406b-368e-40df-a83d-6ecfd8742aa0"],
 " is a function of ",
 Cell[BoxData[
  FormBox["\[Epsilon]", TraditionalForm]],ExpressionUUID->
  "7c892cf0-1f4b-48b8-847e-bf3572bebde0"],
 ". Let\[CloseCurlyQuote]s Taylor-expand this function around ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Epsilon]", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "ac2bd2d7-1e1a-49a2-85b5-6e19cf3ae736"],
 ":\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
     RowBox[{"(", "\[Epsilon]", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
      RowBox[{"(", "0", ")"}]}], "+", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"k", "=", "1"}], "n"], 
      RowBox[{
       SubscriptBox["M", "k"], 
       SuperscriptBox["\[Epsilon]", "k"]}]}], "+", 
     RowBox[{"\[ScriptCapitalO]", "(", 
      SuperscriptBox["\[Epsilon]", 
       RowBox[{"n", "+", "1"}]], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "22340c5f-491c-4a09-9380-207b91bfc251"]
}], "Text",
 CellChangeTimes->{{3.8436494453265457`*^9, 3.843649607834515*^9}, 
   3.84364973872724*^9, {3.843650167222768*^9, 
   3.843650228142603*^9}},ExpressionUUID->"13b07bc4-478d-4945-bb5a-\
bcb6fa6e321a"],

Cell[BoxData[
 RowBox[{
  RowBox[{"M", "[", 
   RowBox[{"\[Epsilon]_", ",", "n_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"M", "[", "0", "]"}], "+", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"k", "=", "1"}], "n"], 
    RowBox[{
     SubscriptBox["M", "k"], 
     SuperscriptBox["\[Epsilon]", "k"]}]}]}]}]], "Input",
 CellChangeTimes->{{3.84364794860148*^9, 3.8436480035572243`*^9}, {
  3.843649115450079*^9, 3.8436491434474173`*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"e86415b8-b20b-45dd-aa77-4404ec0a9962"],

Cell[TextData[{
 "Let\[CloseCurlyQuote]s say we have measured the expectation ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
   TraditionalForm]],ExpressionUUID->"21e40c2e-ed08-4cd8-ae03-e4cc365f7b20"],
 " at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "+", "1"}], TraditionalForm]],ExpressionUUID->
  "a59e43d3-355e-4ca8-ad4d-bc7b8422d160"],
 " different error rates\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"M", "(", 
      RowBox[{
       SubscriptBox["\[Alpha]", "k"], "\[Epsilon]"}], ")"}], 
     "\[RightAngleBracket]"}], ",", " ", 
    RowBox[{"k", "\[Element]", 
     RowBox[{"(", 
      RowBox[{"0", ",", "...", ",", "n"}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"ccffa49d-4250-465e-a25c-3af35d1720e9"],
 " ,\nwhere\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["\[Alpha]", "0"], "<", 
       SubscriptBox["\[Alpha]", "1"], "<"}], "..."}], "<", 
     SubscriptBox["\[Alpha]", "n"]}]}], TraditionalForm]],ExpressionUUID->
  "8cb8d4f8-1e53-4f4d-9952-2b9b294f2f7e"],
 " .\nWith these ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "+", "1"}], TraditionalForm]],ExpressionUUID->
  "18f10a8a-6244-4e70-8527-48ce3334dab0"],
 " different measurements we can fix all coefficients of the Taylor-series up \
to order ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Epsilon]", "n"], TraditionalForm]],ExpressionUUID->
  "b3b62457-aff7-415c-bd48-e624fca75702"],
 ". That is, we can solve for\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
     RowBox[{"(", "0", ")"}]}], ",", " ", 
    SubscriptBox["M", "1"], ",", " ", 
    SubscriptBox["M", "2"], ",", "...", ",", 
    SubscriptBox["M", "n"]}], TraditionalForm]],ExpressionUUID->
  "7af66559-3b0a-491a-bb55-1b3f15ee3f56"],
 " .\nOur estimate for the noiseless expectation value is then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
    RowBox[{"(", "0", ")"}]}], TraditionalForm]],ExpressionUUID->
  "d5c87edd-4435-44dd-bf8e-7ab9123e89b9"],
 "."
}], "Text",
 CellChangeTimes->{{3.843650265985879*^9, 3.843650398803793*^9}, {
  3.843650464014279*^9, 3.843650513253491*^9}, {3.843650561604764*^9, 
  3.843650617290753*^9}},ExpressionUUID->"fd68a948-1a81-40f4-89af-\
057cc1354e1c"],

Cell[CellGroupData[{

Cell[TextData[{
 "Example: ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", "1"}], TraditionalForm]],ExpressionUUID->
  "a1479118-948f-4d0f-ba8a-7cc89ad7f82b"]
}], "Section",
 CellChangeTimes->{{3.843650624856677*^9, 
  3.843650628774122*^9}},ExpressionUUID->"f27135cd-b25c-40f1-bfb5-\
95667b454739"],

Cell[TextData[{
 "We measure\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["\[Alpha]", "0"], "\[Epsilon]"}], ")"}]}], ",", " ", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["\[Alpha]", "1"], "\[Epsilon]"}], ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"b43fdae9-0cf8-4ae4-b78f-e7f4dbd22233"],
 "\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Alpha]", "0"], "=", "1"}], TraditionalForm]],
  ExpressionUUID->"012eb195-4e5c-42cf-b10b-eec4d5314067"],
 " and fit\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
     RowBox[{"(", "0", ")"}]}], ",", " ", 
    SubscriptBox["M", "1"]}], TraditionalForm]],ExpressionUUID->
  "30dffced-1c3e-47be-8d05-7182256f242b"],
 " ."
}], "Text",
 CellChangeTimes->{{3.843650686739058*^9, 
  3.843650793129662*^9}},ExpressionUUID->"b974b871-6d14-45af-9b12-\
aa5592711319"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"M", "[", 
        RowBox[{
         SubscriptBox["\[Alpha]", "0"], "\[Epsilon]"}], "]"}], "\[Equal]", 
       RowBox[{"M", "[", 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Alpha]", "0"], "\[Epsilon]"}], ",", "1"}], "]"}]}], 
      ",", 
      RowBox[{
       RowBox[{"M", "[", 
        RowBox[{
         SubscriptBox["\[Alpha]", "1"], "\[Epsilon]"}], "]"}], "\[Equal]", 
       RowBox[{"M", "[", 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Alpha]", "1"], "\[Epsilon]"}], ",", "1"}], 
        "]"}]}]}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"M", "[", "0", "]"}], ",", 
      SubscriptBox["M", "1"]}], "}"}]}], "]"}], "//", "Expand"}]], "Input",
 CellChangeTimes->{{3.843648128823031*^9, 3.843648163584034*^9}, {
  3.843648242602261*^9, 3.8436482452093697`*^9}, {3.8436484008514*^9, 
  3.843648510663937*^9}, {3.8436491246699247`*^9, 3.843649152469159*^9}, {
  3.843649352719014*^9, 3.843649389593404*^9}, {3.843655165776211*^9, 
  3.843655166777232*^9}},
 CellLabel->"In[2]:=",ExpressionUUID->"68c93bb0-109c-4f55-9ca2-168055a5842b"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"M", "[", "0", "]"}], "\[Rule]", 
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"M", "[", 
         RowBox[{"\[Epsilon]", " ", 
          SubscriptBox["\[Alpha]", "1"]}], "]"}], " ", 
        SubscriptBox["\[Alpha]", "0"]}], 
       RowBox[{
        SubscriptBox["\[Alpha]", "0"], "-", 
        SubscriptBox["\[Alpha]", "1"]}]], "-", 
      FractionBox[
       RowBox[{
        RowBox[{"M", "[", 
         RowBox[{"\[Epsilon]", " ", 
          SubscriptBox["\[Alpha]", "0"]}], "]"}], " ", 
        SubscriptBox["\[Alpha]", "1"]}], 
       RowBox[{
        SubscriptBox["\[Alpha]", "0"], "-", 
        SubscriptBox["\[Alpha]", "1"]}]]}]}], ",", 
    RowBox[{
     SubscriptBox["M", "1"], "\[Rule]", 
     RowBox[{
      FractionBox[
       RowBox[{"M", "[", 
        RowBox[{"\[Epsilon]", " ", 
         SubscriptBox["\[Alpha]", "0"]}], "]"}], 
       RowBox[{"\[Epsilon]", " ", 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["\[Alpha]", "0"], "-", 
          SubscriptBox["\[Alpha]", "1"]}], ")"}]}]], "-", 
      FractionBox[
       RowBox[{"M", "[", 
        RowBox[{"\[Epsilon]", " ", 
         SubscriptBox["\[Alpha]", "1"]}], "]"}], 
       RowBox[{"\[Epsilon]", " ", 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["\[Alpha]", "0"], "-", 
          SubscriptBox["\[Alpha]", "1"]}], ")"}]}]]}]}]}], "}"}], 
  "}"}]], "Output",
 CellChangeTimes->{{3.843648419419277*^9, 3.8436485110451593`*^9}, {
   3.843649121100216*^9, 3.8436491530397167`*^9}, {3.843649360347581*^9, 
   3.843649389914678*^9}, {3.843650871813067*^9, 3.843650876116827*^9}, 
   3.8436551670334187`*^9, 3.843658354411873*^9, 3.8612627729433503`*^9},
 CellLabel->"Out[2]=",ExpressionUUID->"7b916053-2e71-4e3b-a53d-347a1d21168b"]
}, Open  ]],

Cell[TextData[{
 "Therefore,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
     RowBox[{"(", "0", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       SubscriptBox["\[Alpha]", "1"], 
       RowBox[{
        SubscriptBox["\[Alpha]", "1"], "-", 
        SubscriptBox["\[Alpha]", "0"]}]], 
      RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Alpha]", "0"], "\[Epsilon]"}], ")"}]}], "+", 
     RowBox[{
      FractionBox[
       SubscriptBox["\[Alpha]", "0"], 
       RowBox[{
        SubscriptBox["\[Alpha]", "0"], "-", 
        SubscriptBox["\[Alpha]", "1"]}]], 
      RowBox[{"\[LeftAngleBracket]", "M", "\[RightAngleBracket]"}], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Alpha]", "1"], "\[Epsilon]"}], ")"}]}], " ", "+", " ", 
     RowBox[{"\[ScriptCapitalO]", "(", 
      SuperscriptBox["\[Epsilon]", "2"], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"24e8d619-bac8-4ee5-8277-e1b06ca4590c"],
 " ."
}], "Text",
 CellChangeTimes->{{3.843650824885007*^9, 
  3.843650939870446*^9}},ExpressionUUID->"5c78617e-9402-42de-bfa1-\
4865e2667723"],

