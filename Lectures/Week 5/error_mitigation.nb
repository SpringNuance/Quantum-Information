284fc377d357"],

Cell[TextData[{
 "Consider measuring ",
 Cell[BoxData[
  FormBox["X", TraditionalForm]],ExpressionUUID->
  "d133fd53-7984-4b31-aaae-dea997fff21b"],
 " on the state ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[Rho]", "=", 
      RowBox[{"|", "+"}]}], "\[RightAngleBracket]"}], 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"+", "|"}]}]}], TraditionalForm]],ExpressionUUID->
  "a09ad4cb-9b32-49d3-83eb-1a2fd69b73e6"],
 " under a depolarizing noise channel\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalE]", "(", "\[Rho]", ")"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["\[Epsilon]", "2"], "I"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", "\[Epsilon]"}], ")"}], "\[Rho]"}]}]}], 
   TraditionalForm]],ExpressionUUID->"ce68726e-6c1a-4f71-b5f9-76fd5c8276e2"],
 " .\nThe expectation values are measured for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Alpha]", "0"], "=", "1"}], TraditionalForm]],
  ExpressionUUID->"f548f9d2-c162-486c-a74d-4035753fbad4"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Alpha]", "1"], "=", "2"}], TraditionalForm]],
  ExpressionUUID->"9491f85e-9306-4694-8dce-6fb76cb67e47"],
